{
    "kind": "Service",
    "metadata": {
        "labels": {
            "external": "false",
            "{{.NS_SELECTOR}}": "{{.DB_NAME}}-{{.HASH}}"
        },
        "name": "{{.DB_NAME}}-{{.HASH}}"
    },
    "spec": {
        "selector": {
            "{{.NS_SELECTOR}}": "{{.DB_NAME}}-{{.HASH}}"
        },
        "ports": [
            {
                "protocol": "TCP",
                "targetPort": {{.DB_PORT}},
                "port": {{.DB_PORT}},
                "name": "{{.DB_NAME}}"
            }
        ]
    }
}
